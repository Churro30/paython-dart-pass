Q2\

void main()
{
string animal1 = dog;
sring animal2  cat;
string animal3 = cow;
}

abstract class animal {
void printName();
void printSound();
}
class dog extends animal {
void printName() {
print("DOG");
}
void printSound(){
print("sound is high");
}
class cat extends animal {
void printName() {
print("CAT");
}
void printSound(){
print("sound is low");
}
class cow extends animal {
void printName() {
print("COW");
}
void printSound(){
print("sound is loud");
}

